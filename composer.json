{
  "autoload": {
    "psr-4": {
      "Controller\\": "src/Controller",
      "Core\\": "src/Core",
      "Model\\": "src/Model",
      "View\\": "src/View"
    }
  },
  "require": {
    "texlab/mydb": "^0.0.4",
    "texlab/html": "^0.03.0"
    
  },
  "require-dev": {
    "vimeo/psalm": "^3.12",
    "phpstan/phpstan": "^0.12.33",
    "squizlabs/php_codesniffer": "3.*"
  },
  "scripts": {
    "idb": "php install/install.php",
    "rs": "php -S localhost:8000 -t public/",
    "psalm": "psalm",
    "phpstan": "phpstan analyse -c phpstan.neon",
    "codestyle": "phpcs",
    "codestylefix": "phpcbf"
  }
}